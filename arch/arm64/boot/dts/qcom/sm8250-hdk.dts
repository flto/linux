	status = "okay";
	fsa4480: fsa4480@43 {
		compatible = "fairchild,fsa4480";
		reg = <0x43>;
		mode-switch;
		svid = /bits/ 16 <0xff01>;
		port {
			fsa_mode: endpoint@0 {
				remote-endpoint = <&fsa_mode_out>;
			};
		};
	};
};
